\\globals\\colors.scss":":root {\r\n    --primary-bright-blue: hsl(243, 87%, 12%);\r\n    --primary-desaturated-blue: hsl(238, 22%, 44%);\r\n\r\n    --accent-bright-blue: hsl(224, 93%, 58%);\r\n    --accent-moderate-cyan: hsl(170, 45%, 43%);\r\n\r\n    --neutral-light-greyish-blue: hsl(240, 75%, 98%);\r\n    --neutral-light-gray: hsl(0, 0%, 75%);\r\n}","sass\\globals\\boilerplate.scss":"html {\r\n    font-size: 100%;\r\n    box-sizing: border-box;\r\n}\r\n\r\n*, *::before, *::after {\r\n    box-sizing: inherit;\r\n}\r\n\r\nbody {\r\n    margin: 0;\r\n    padding: 0;\r\n    font-family: var(--font-raleway);\r\n}\r\n\r\nfieldset {\r\n    border: none;\r\n    padding: 0;\r\n}","sass\\globals\\fonts.scss":":root {\r\n    --font-raleway: 'Raleway', sans-serif;\r\n    --font-open-sans: 'Open Sans', sans-serif;\r\n}","sass\\layout\\grid.scss":"@use '../util' as *;\r\n\r\n.grid {\r\n    display: grid;\r\n    gap: rem(25);\r\n\r\n    @include breakpoint(large) {\r\n        grid-template-columns: repeat(2, 1fr);\r\n    }\r\n}","sass\\util\\breakpoints.scss":"// 640px, 1150x, 1400px\r\n$breakpoints-up: (\r\n    \"medium\": '40em',\r\n    \"large\": '71.875em',\r\n    \"xlarge\": '87.5em'\r\n);\r\n\r\n// 639.98px, 1149.98px, 1439.98px\r\n$breakpoints-down: (\r\n    'small': '39.9375em',\r\n    'medium': '71.87375em',\r\n    'large': '87.4375em'\r\n);\r\n\r\n@mixin breakpoint($size) {\r\n    @media only screen and (min-width: map-get($breakpoints-up, $size)) {\r\n        @content;\r\n    }\r\n}\r\n\r\n@mixin breakpoint-down($size) {\r\n    @media only screen and (max-width: map-get($breakpoints-down, $size)) {\r\n        @content;\r\n    }\r\n}","sass\\components\\header.scss":"@use '../util' as *;\r\n\r\n.nav {\r\n    height: rem(100);\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    max-width: rem(1280);\r\n    margin: 0 auto rem(30);\r\n\r\n    &__logo {\r\n        height: rem(30);\r\n        width: rem(100);\r\n        margin-left: rem(25);\r\n    }\r\n\r\n    &__links {\r\n        display: flex;\r\n        list-style: none;\r\n        margin-right: rem(30);\r\n        padding-left: 0;\r\n    }\r\n\r\n    &__links li {\r\n        padding: 0 clamp(rem(1), 1.5vw, rem(55));\r\n    }\r\n\r\n    &__links a {\r\n        color: hsl(240, 100%, 3%);\r\n        font-size: rem(14);\r\n    }\r\n}","sass\\components\\buttons.scss":"@use '../util' as *;\r\n\r\n.btn {\r\n    width: 100%;\r\n    height: rem(50);\r\n    background-color: var(--accent-bright-blue);\r\n    color: white;\r\n    font-weight: 700;\r\n    font-size: rem(18);\r\n    border: none;\r\n    border-radius: 5px;\r\n\r\n    @include breakpoint(medium) {\r\n        width: rem(200);\r\n    }\r\n}","sass\\components\\hero.scss":"@use '../util' as *;\r\n\r\n.hero {\r\n    // @include breakpoint(large) {\r\n    //     margin: rem(20);\r\n    // }\r\n    margin: 0 auto;\r\n    padding: rem(15);\r\n    max-width: rem(1000);\r\n\r\n    &__illustration {\r\n        width: 85%;\r\n        margin: 0 auto;\r\n        // margin: 0 rem(12) 0;\r\n        @include breakpoint(large) {\r\n            order: 2;\r\n        }\r\n    }\r\n\r\n    &__heading {\r\n        text-align: center;\r\n        line-height: 1.5;\r\n        margin: 0 rem(8) rem(26);\r\n\r\n        @include breakpoint(large) {\r\n            text-align: left;\r\n        }\r\n    }\r\n\r\n    &__info {\r\n        text-align: center;\r\n        line-height: 1.5;\r\n\r\n        @include breakpoint(large) {\r\n            text-align: left;\r\n        }\r\n    }\r\n\r\n    &__fieldset {\r\n        text-align: center;\r\n\r\n        @include breakpoint(medium) {\r\n            margin: 0 auto;\r\n        }\r\n\r\n        @include breakpoint(large) {\r\n            text-align: left;\r\n        }\r\n    }\r\n\r\n    &__email-input {\r\n        border-radius: rem(5);\r\n        color: var(--neutral-light-gray);\r\n        height: rem(45);\r\n        width: 100%;\r\n        border: rem(1.5) solid black;\r\n        font-size: rem(16);\r\n        color: var(--neutral-light-gray);\r\n        padding-left: rem(20);\r\n        margin-top: rem(40);\r\n        margin-bottom: rem(18);\r\n\r\n        @include breakpoint(medium) {\r\n            width: rem(290);\r\n        }\r\n    }\r\n\r\n    &__btn {\r\n        box-shadow: rem(2) rem(2) rem(3) rgba(0, 0, 0, 0.2);\r\n\r\n        @include breakpoint(medium) {\r\n            margin-left: rem(10);\r\n        }\r\n\r\n        @include breakpoint(large) {\r\n            margin-left: 0;\r\n        }\r\n    }\r\n\r\n    &__container {\r\n        max-width: 80vw;\r\n        margin: 0 auto;\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"68579e8233437bffccbd31318ebe3dbb","cacheData":{"env":{}}}